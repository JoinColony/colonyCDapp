{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
       "~constants": ["src/constants"],
       "~constants/*": ["src/constants/*"],
    //   "~externalUrls": ["src/modules/externalUrls.ts"],
    //   "~admin/*": ["src/modules/admin/components/*"],
      "~context": ["src/context"],
      "~context/*": ["src/context/*"],
      "~shared/*": ["src/components/shared/*"],
      "~common/*": ["src/components/common/*"],
      "~root/*": ["src/components/root/*"],
      "~gql": ["src/graphql"],
      "~gql/*": ["src/graphql/*"],
    //   "~dashboard/*": ["src/modules/dashboard/components/*"],
    //   "~data/*": ["src/data/*"],
    //   "~dialogs/*": ["src/modules/dashboard/components/Dialogs/*"],
    //   "~lib/*": ["src/lib/*"],
    //   "~pages/*": ["src/modules/pages/components/*"],
      "~redux": ["src/redux"],
      "~redux/*": ["src/redux/*"],
    //   "~routes/*": ["src/routes/*"],
      "~styles/*": ["src/styles/shared/*"],
    //   "~testutils": ["src/__tests__/utils.ts"],
    //   "~modules/*": ["src/modules/*"],
      "~types": ["src/types"],
      "~types/*": ["src/types/*"],
    //   "~users/*": ["src/modules/users/components/*"],
      "~utils/*": ["src/utils/*"]
    },
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["dom", "es2019"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmit": true,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "target": "es6",
    "typeRoots": ["src/types/definitions", "node_modules/@types"],
    "types": ["node", "jest"]
  }
}
